<h1>Listing feeds</h1>
 
<table>
  <tr>
    <th>Id</th>
    <th>Title</th>
    <th>URL</th>
  </tr>
 
  <% @feeds.each do |feed| %>
    <tr>
      <td><%= feed.id %></td>
      <td><%= feed.title %></td>
      <td><%= feed.url %></td>
      <td><%= link_to 'Show', feed_path(feed) %></td>
      <td><%= link_to 'Remove', feed_path(feed),
                                 method: :delete,
                                 data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
